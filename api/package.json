    "Caryn Swartz",
    "Jen Thibault",
    "Greg Walker",
    "Nichole Weems",
    "Michael Knoll",
    "Darren Mirano",
    "Chris Miraflor"
  ],
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/CMSgov/eAPD/issues"
  },
  "homepage": "https://github.com/CMSgov/eAPD#readme",
  "dependencies": {
    "@okta/jwt-verifier": "^2.1.0",
    "@okta/okta-sdk-nodejs": "^4.5.1",
    "ajv": "^8.1.0",
    "aws-sdk": "^2.888.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "jsonpatch": "^3.0.1",
    "jsonpatch-to-mongodb": "^1.0.0",
    "jsonpointer": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "migrate-mongoose": "^4.0.0",
    "mongoose": "^5.12.11",
    "mongoose-seed": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "sanitize-html": "^2.3.3",
    "sharp": "^0.28.1",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "zxcvbn": "^4.4.2"
  },
  "engines": {
    "node": "^14.16.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sinon": "^10.0.0",
    "supertest": "^6.1.3",
    "tap": "^15.0.2"
  },
  "resolutions": {
    "browserslist": "4.16.5",
    "hosted-git-info": "4.0.2",
    "path-parse": "1.0.7"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test|endpoint).js?(x)"
    ],
    "testEnvironment": "node"
  }
}
